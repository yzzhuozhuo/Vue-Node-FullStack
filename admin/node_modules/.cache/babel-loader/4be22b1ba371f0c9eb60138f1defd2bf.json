v class=\"categoryList\">\r\n    <h1>物品列表</h1>\r\n    <el-table :data=\"items\">\r\n      <el-table-column prop=\"_id\" label=\"ID\" width=\"240\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"物品名称\" width=\"300\"></el-table-column>\r\n      <el-table-column prop=\"icon\" label=\"图标\">\r\n        <template slot-scope=\"scope\">\r\n          <img :src=\"scope.row.icon\" alt=\"\" style=\"height:3rm\">\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\r\n        <template v-slot=\"scope\">\r\n          <el-button\r\n            type=\"text\"\r\n            size=\"small\"\r\n            @click=\"$router.push(`/items/edit/${scope.row._id}`)\"\r\n          >编辑</el-button>\r\n          <el-button\r\n            type=\"text\"\r\n            size=\"small\"\r\n            @click=\"remove(scope.row)\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      items: [],\r\n      res: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async fetch() {\r\n      const res = await this.$http.get(\"rest/items\");\r\n      this.items = res.data;\r\n    },\r\n    async remove(row) {\r\n      this.$confirm(`是否确定删除分类\"${row.name}\"`, \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(async () => {\r\n          const res = await this.$http.delete(`rest/items/${row._id}`)\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\"\r\n          })\r\n          this.res = res\r\n          this.fetch()\r\n       })\r\n    }\r\n  },\r\n  created() {\r\n    this.fetch();\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}