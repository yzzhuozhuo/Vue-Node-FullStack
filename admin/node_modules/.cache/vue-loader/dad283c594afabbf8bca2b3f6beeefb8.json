ck=\"remove(scope.row)\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      items: [],\r\n      res: ''\r\n    };\r\n  },\r\n  methods: {\r\n    async fetch() {\r\n      const res = await this.$http.get(\"rest/items\");\r\n      this.items = res.data;\r\n    },\r\n    async remove(row) {\r\n      this.$confirm(`是否确定删除分类\"${row.name}\"`, \"提示\", {\r\n        confirmButtonText: \"确定\",\r\n        cancelButtonText: \"取消\",\r\n        type: \"warning\"\r\n      })\r\n        .then(async () => {\r\n          const res = await this.$http.delete(`rest/items/${row._id}`)\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"删除成功\"\r\n          })\r\n          this.res = res\r\n          this.fetch()\r\n       })\r\n    }\r\n  },\r\n  created() {\r\n    this.fetch();\r\n  }\r\n};\r\n</script>"]}]}